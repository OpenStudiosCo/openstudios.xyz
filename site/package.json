{
  "name": "openstudios.xyz",
  "author": "Open Studios < team@openstudios.xyz >",
  "version": "3.0.0",
  "description": "The homepage of Open Studios and a Kettlefish inspired Eleventy boilerplate",
  "engines": {
    "npm": ">=10.0.0 <11.0.0",
    "node": ">=22.0.0 <23.0.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production npm-run-all clean -p build:*",
    "build:11ty": "eleventy --quiet",
    "build:stylus": "node_modules/.bin/stylus src/styles/1999.styl --out web/assets/css/1999.css",
    "clean": "rimraf dist",
    "dev": "npm-run-all clean build:* -p dev:*",
    "dev:11ty": "eleventy --serve --quiet",
    "dev:stylus": "node_modules/.bin/stylus -w src/styles/1999.styl --out web/assets/css/1999.css",
    "preinstall": "npm install stylus -g"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-fetch": "^4.0.1",
    "axios": "^1.7.7",
    "cross-env": "^7.0.3",
    "jsdom": "^25.0.1",
    "jstransformer-markdown-it": "^3.0.0",
    "keen-slider": "^6.8.6",
    "npm-run-all": "^4.1.5",
    "parvus": "^2.6.0",
    "prismjs": "^1.29.0",
    "pug": "^3.0.3",
    "stylus": "^0.63.0"
  }
}
